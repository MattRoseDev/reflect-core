scalar Time

input LoginInput {
  username: String!
  password: String!
}

input RegisterInput {
  email: String!
  username: String!
  password: String!
}

type Query {
  login(input: LoginInput): AuthOutput!
}

type Mutation {
  register(input: RegisterInput): AuthOutput!
}

type AuthOutput {
  token: String!
  user: User
}
